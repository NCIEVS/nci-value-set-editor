<root>

    <ConfigurableItems>
        <ConfigurableItem>
             <name>DEBUG</name>
             <value>@debug@</value>
        </ConfigurableItem>

        <ConfigurableItem>
             <name>BUILD_DATE</name>
             <value>@build.date@</value>
        </ConfigurableItem>

        <ConfigurableItem>
             <name>APPLICATION_VERSION</name>
             <value>@application.version@</value>
        </ConfigurableItem>

        <ConfigurableItem>
             <name>APP_BUILD_TAG</name>
             <value>@app.build.tag@</value>
        </ConfigurableItem>

        <ConfigurableItem>
             <name>EVS_SERVICE_URL</name>
             <value>@lex.evs.api.url@</value>
        </ConfigurableItem>

        <ConfigurableItem>
             <name>NCIT_URL</name>
             <value>@ncit.url@</value>
        </ConfigurableItem>
        
        
        <ConfigurableItem>
             <name>LG_CONFIG_FILE</name>
             <value>@lg.config.file@</value>
        </ConfigurableItem>


        <ConfigurableItem>
                 <name>EHCACHE_XML_PATHNAME</name>
                 <value>@ehcache.xml.file@</value>
        </ConfigurableItem>
        
        
    </ConfigurableItems>

    <SecurityTokenHolders>
        <SecurityTokenHolder>
             <name>MedDRA (Medical Dictionary for Regulatory Activities Terminology)</name>
             <value>@meddra_security_token@</value>
        </SecurityTokenHolder>
    </SecurityTokenHolders>

</root>
